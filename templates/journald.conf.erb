<% if @seal -%>
Seal=<%= @seal %>
<% end -%>
<% if @split_mode -%>
SplitMode=<%= @split_mode %>
<% end -%>
<% if @sync_interval_sec -%>
SyncIntervalSec=<%= @sync_interval_sec %>
<% end -%>
<% if @rate_limit_interval_sec -%>
